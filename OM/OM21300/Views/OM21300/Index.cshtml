@using Ext.Net;
@using Ext.Net.MVC;
@using HQ.eSkyFramework;
@using HQ.eSkyFramework.HQControl;
@using OM21300;

@model System.Collections.IEnumerable
@{
    Layout = null;
    var X = Html.X();
    var HQ = Html.HQ();
    ViewBag.Title = "OM21300";
    var acc = Session["OM21300"] as AccessRight;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <script src='@Url.Content("Scripts/hq.language"+Current.LangID+".js?"+DateTime.Now.ToString("yyyyMMddss"))' type="text/javascript"></script>
    <script src='@Url.Content("Scripts/hq.common.js?"+DateTime.Now.ToString("yyyyMMddss"))' type="text/javascript"></script>
    <script src='@Url.Content("Scripts/markerclusterer.js?")' type="text/javascript"></script>
    <script src='@Url.Content("Scripts/convex_hull.js?")' type="text/javascript"></script>
    <script src='@Url.Content("Scripts/jquery-1.7.1.min.js?")' type="text/javascript"></script>

   <script src='@Url.Content("Scripts/screen/OM21300.js?"+DateTime.Now.ToString("yyyyMMddss"))' type="text/javascript"></script>
    <script src="https://maps.googleapis.com/maps/api/js?v=3&key=AIzaSyD1KCqJiFRTtXsU2UxG3Zf7D55Gjft7JJI&libraries=drawing&language=vi" type="text/javascript"></script>
    
     <script src='@Url.Content("Scripts/screen/map.country.js")' type="text/javascript"></script>
    <script src='@Url.Content("Scripts/screen/map.city.js")' type="text/javascript"></script>
   
     <style>
        .x-grid-row-over .x-grid-cell-inner {
            font-weight : bold;
        }
        .validated-row .x-grid-cell, .dirty-row .x-grid-rowwrap-div {
	        background-color: #BBFFA9 !important;
        }
        .scan-row .x-grid-cell, .dirty-row .x-grid-rowwrap-div {
	        background-color: #A9BDFF !important;
        }
        .tree-node-noicon {
            display:none !important;
        }

        .popup-label {
            font-weight:bold;
        }
        .hq-mcombo-item-checked td:first-child {
            background-image: url("extnet/resources/images/grid/checked-gif/ext.axd") !important;
            background-position: center !important;
            background-repeat: no-repeat !important;
            background-color: transparent!important;
            padding-left: 18px !important;
        }

        .hq-mcombo-item-unchecked td:first-child{
            background-image: url("extnet/resources/images/grid/unchecked-gif/ext.axd");
            background-position: center;
            background-repeat: no-repeat;
            background-color: transparent;
            padding-left: 18px;
        }
        .tree-parent-icon {
            height: 24px !important;
            width: 24px !important;
            background-size: 24px 24px;
        }

        .tree-node-parent span{
            font-size: 12px !important;
            font-weight: normal;
            line-height:24px !important;
        }
    </style>
    <script>

        HQ.cpnyID = '@Current.CpnyID';
        HQ.langID = '@Current.LangID';
        HQ.userName = '@Current.UserName';
        HQ.businessDate = new Date();
        HQ.isRelease = HQ.util.toBool('@acc.Release.ToString()');
        HQ.isUpdate = HQ.util.toBool('@acc.Update.ToString()');
        HQ.isInsert = HQ.util.toBool('@acc.Insert.ToString()');
        HQ.isDelete = HQ.util.toBool('@acc.Delete.ToString()');
        HQ.role = '@ViewBag.Role'
    </script>  
    <title>Phân Tích Bao Phủ</title>
</head>
<body>   
@(
    X.ResourceManager().Theme((Theme)Enum.Parse(typeof(Theme),Current.Theme.PassNull()==""?"Gray":Current.Theme.PassNull()))
)
@(X.StatusBar())
@(X.GridFilters())

@{ Html.RenderAction("Body", new { lang = Current.LangID.ToString() }); }
</body>
</html>
 